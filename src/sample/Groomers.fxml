<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="sample.Groomers">
  <children>
    <MenuButton alignment="CENTER" contentDisplay="RIGHT" layoutX="30.0" layoutY="30.0"
      mnemonicParsing="false" text="Groomer" AnchorPane.bottomAnchor="365.0"
      AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <font>
        <Font size="18.0"/>
      </font>
      <items>
        <MenuItem fx:id="menuItem_HomePage" mnemonicParsing="false" text="Home"/>
        <Menu mnemonicParsing="false" text="Adoptable">
          <items>
            <MenuItem fx:id="menuItem_AdoptableDogs" mnemonicParsing="false" text="Adoptable Dogs"/>
            <MenuItem fx:id="menuItem_AdoptableCats" mnemonicParsing="false" text="Adoptable Cats"/>
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Animal Info">
          <items>
            <MenuItem fx:id="menuItemEditAnimal" mnemonicParsing="false" text="Edit Animal Info"/>
            <MenuItem fx:id="menuItem_SearchAnimal" mnemonicParsing="false" text="Search Animal "/>
            <MenuItem fx:id="menuItem_AnimalStatus" mnemonicParsing="false" text="Animal Status"/>
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Animal Care">
          <items>
            <MenuItem fx:id="menuItem_Groomers" mnemonicParsing="false" text="Groomers"/>
            <MenuItem fx:id="menuItem_VetVisit" mnemonicParsing="false" text="Vet Visit"/>
          </items>
        </Menu>
      </items>
    </MenuButton>
    <GridPane layoutX="128.0" layoutY="115.0" prefHeight="170.0" prefWidth="345.0">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      </rowConstraints>
      <children>
        <Label text="Teeth" GridPane.rowIndex="1"/>
        <Label text="Nails Trimmed" GridPane.rowIndex="2"/>
        <Label text="Fur Trimmed" GridPane.rowIndex="3"/>
        <Label text="Ear Cleaning" GridPane.rowIndex="4"/>
        <Label text="Shampoo"/>
        <ComboBox fx:id="comboBox_Shampoo" prefWidth="150.0" GridPane.columnIndex="1"/>
        <ComboBox fx:id="comboBox_Teeth" prefWidth="150.0" GridPane.columnIndex="1"
          GridPane.rowIndex="1"/>
        <ComboBox fx:id="comboBox_NailsTrimmed" prefWidth="150.0" GridPane.columnIndex="1"
          GridPane.rowIndex="2"/>
        <ComboBox fx:id="comboBox_FurTrimmed" prefWidth="150.0" GridPane.columnIndex="1"
          GridPane.rowIndex="3"/>
        <ComboBox fx:id="comboBox_EarCleaning" prefWidth="150.0" GridPane.columnIndex="1"
          GridPane.rowIndex="4"/>
      </children>
    </GridPane>
    <Button fx:id="comboBox_Enter" layoutX="399.0" layoutY="330.0" mnemonicParsing="false"
      text="Enter"/>
  </children>
</AnchorPane>
